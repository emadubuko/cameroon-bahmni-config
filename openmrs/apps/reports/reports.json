    "Myvisitreport": {
        "name": "Visit Report",
        "type": "concatenated",
        "config": {
            "macroTemplatePath": "/var/www/bahmni_config/openmrs/apps/reports/VISIT_REPORT/template/visit_report.xls",
            "reports": [    
               {
                    "name": "visit",
                    "type": "visits",
                    "config": {
                    
                        "forDataAnalysis": false,
                        "patientAttributes": [
                            "PERSON_ATTRIBUTE_TYPE_PHONE_NUMBER",
                            "education",
                            "occupation",
                            "matrimonialStatus",
                            "emergencyContact"
                        ],
                        "patientAddresses": [
                            "address1",
                            "address2",
                            "city_Village",
                            "county_District",
                            "address3",
                            "state_Province"
                        ],
                        "additionalPatientIdentifiers": ["REGISTRATION_IDTYPE_1_CNI_KEY","REGISTRATION_IDTYPE_3_ANC_KEY","REGISTRATION_IDTYPE_2_ART_KEY"],
                        "applyDateRangeFor": "visitStopDate",
                        "excludeColumns": ["Date Of Admission","Date Of Discharge","New patient visit"],
                        "preferredColumns": ["Patient Identifier","Patient Name","Age","Birthdate","Gender","Patient Created Date","Visit Type","Date started","Date stopped","PERSON_ATTRIBUTE_TYPE_PHONE_NUMBER","address1","address2","city_Village","county_District","address3","state_Province","education","occupation","matrimonialStatus","emergencyContact","REGISTRATION_IDTYPE_3_ANC_KEY","REGISTRATION_IDTYPE_2_ART_KEY","REGISTRATION_IDTYPE_1_CNI_KEY"],
                        "visitTypesToFilter": []
                    }
                },
                {
                    "name": "ReportHospitalRegion",
                    "type": "MRSGeneric",
                    "config": {
                        "sqlPath": "/var/www/bahmni_config/openmrs/apps/reports/VISIT_REPORT/sql/hospital_region.sql"
                    }
                },
                {
                    "name": "ReportHospitalDistrict",
                    "type": "MRSGeneric",
                    "config": {
                        "sqlPath": "/var/www/bahmni_config/openmrs/apps/reports/VISIT_REPORT/sql/hospital_district.sql"
                    }
                },                            
                {
                    "name": "ReportHospitalName",
                    "type": "MRSGeneric",
                    "config": {
                        "sqlPath": "/var/www/bahmni_config/openmrs/apps/reports/VISIT_REPORT/sql/hospital_name.sql"
                    }
                }
            ]
        }
    },
