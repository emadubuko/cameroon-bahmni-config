    "pivotReportOfVisits": {
        "name": "Aggregation report for visits",
        "type": "aggregation",
        "config": {
            "report":{
                "type": "visits",
                "config": {
                    "forDataAnalysis": true,
                    "patientAttributes": [
                        "caste",
                        "class",
                        "education",
                        "occupation",
                        "primaryContact"
                    ],
                    "visitAttributes": [
                        "Visit Status",
                        "Admission Status"
                    ],
                    "patientAddresses": [
                        "address3",
                        "city_village"
                    ],
                    "applyDateRangeFor": "visitStopDate",
                    "visitTypesToFilter": [
                        "PHARMACY VISIT",
                        "OPD"
                    ]
                }
            },
            "rowGroups": [
                "Gender"
            ],
            "columnGroups": [
                "Visit Status"
            ],
            "distinctGroups": [
                "Patient Identifier"
            ]
        }
    },

