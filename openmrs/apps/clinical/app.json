{
    "id": "bahmni.clinical",
    "extensionPoints": [
        {
            "id": "org.bahmni.clinical.consultation.board",
            "description": "Bahmni Clinical Consultation Page"
        },
        {
            "id": "org.bahmni.clinical.conceptSetGroup.*",
            "description": "Define conceptsets part of a conceptset group"
        },
        {
            "id": "org.bahmni.clinical.orders",
            "description": "Bahmni Orders Page"
        },
        {
            "id": "org.bahmni.patient.search",
            "description": "Bahmni Patient search, lookups Page"
        }
    ],
    "contextModel": [
        "patientUuid",
        "visitUuid"
    ],
    "config": {
        "hideConditions": false,
        "enableRadiologyOrderOptions":["Urgent","NeedsPrint"],
        "quickPrints":false,
        "networkConnectivity" : {
            "showNetworkStatusMessage": false,
            "networkStatusCheckInterval": 20000,
            "locales" : [
                { "locale" : "fr", "css" : "offline-language-french"},
                { "locale" : "es", "css": "offline-language-spanish"},
                { "locale" : "pt", "css": "offline-language-portuguese-brazil"}
            ]
        },
    	"diagnosisStatus": "Inactive",
        "program": {
            "patientInformation": {
                "ageLimit": 100
            },
            "PROGRAM_MANAGEMENT_3_PATIENT_COMORBIDITES": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_DEFAULTERS_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_6_LABEL_THERAPEUTIC_LINE": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_DEFAULTERS_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_1_DURATION_ON_ART": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_5_PATIENT_STAGE": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_DEFAULTERS_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_4_PATIENT_DOCTOR_IN_CHARGE": {
                "excludeFrom": ["HIV_DEFAULTERS_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_1_ART_NUMBER": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_DEFAULTERS_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_7_REASON_FOR_DEFAULTING": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_2_LAST_FAILED_APPOINTMENT_DATE": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_3_PRETRACKING_OUTCOME": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_8_RETURNED_TO_TREATMENT_DATE": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_4_TRACKING_DATE": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"],
                "required" : true
            },
            "PROGRAM_MANAGEMENT_5_APS_NAME": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            },
            "PROGRAM_MANAGEMENT_6_TRACKING_OUTCOME": {
                "excludeFrom": ["TB_PROGRAM_KEY", "HIV_PROGRAM_KEY"]
            }
        },
        "patientContext": {
        	"programAttributes": [
                "Id_Number",
                "Doctor",
                "Stage"
        	],
        	"personAttributes": [
                "class",
                "caste"
            ],
            "additionalPatientIdentifiers": [
                "National ID"
       		],
        	"preferredIdentifier": "Id_Number"
    	},
        "obsIgnoreList": [
            "REGISTRATION FEES",
            "COMMENTS",
            "Patient file"
        ],
        "otherInvestigationsMap": {
            "Radiology": "Radiology Order",
            "Endoscopy": "Endoscopy Order"
        },
        "allowConsultationWhenNoOpenVisit": false,
        "defaultVisitType": "OPD",
        "visitTypeForRetrospectiveEntries": "Special OPD",
        "maxConceptSetLevels": 3,
        "allowOnlyCodedDiagnosis": false,
        "conceptSetUI": {
            "Chief Complaint Data": {
                "autocomplete":true,
                "codedConceptName":"Chief Complaint",
                "nonCodedConceptName":"Non-Coded Chief Complaint",
                "durationRequired": false,
                "allowAddMore": true
            },
            "Tuberculosis, Adverse Effects" :{
                "multiSelect" : true,
                "autocomplete" : true
            },
            "Posture": {
                "buttonSelect": true
            },
            "P/A Presenting Part": {
                "multiSelect": true
            },
            "FHS": {
                "buttonSelect": true
            },
            "P/S (Per Speculum) - Cervix": {
                "grid": true
            },
            "Anaemia, Stopped Since When": {
                "conciseText": true
            },
            "Examination Notes": {
                "conciseText": true
            },
            "Set1": {
                "allowAddMore": true
            },
            "Set2": {
                "allowAddMore": true
            },
            "Coded3": {
                "multiSelect": true
            },
            "Image": {
                "allowAddMore": true
            },
            "Gynaecology, Gravida": {
                "stepper": true
            },
            "Accepted Family Planning methods": {
                "multiSelect": true
            },
            "Appointment date": {
                "allowFutureDates": true
            },
            "defaults": {
                "Number of living children": 1,
                "Accepted Family Planning methods": [
                    "Condoms",
                    "Pills"
                ],
                "Pain management given": true,
                "Safe Abortion, Remarks": "Remarks",
                "Malaria, Death Date": "2015-12-01"
            }
        },
        "visitPage": {
            "observationDisplay": {
                "conceptNames": []
            }
        }
    }
}
